<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wavemaker.jasper.dao.Employee" table="EMPLOYEE">
        <id name="employeeId" column="ID">
            <generator class="increment"/>
        </id>

        <property name="name" type="java.lang.String" column="NAME"/>
        <property name="department" type="java.lang.String" column="DEPARTMENT"/>
        <property name="emailId" type="java.lang.String" column="EMAIL"/>
        <property name="noOfStocks" type="java.lang.Integer" column="STOCKS"/>
    </class>
</hibernate-mapping>
